# ClickHouse Connection
CLICKHOUSE_HOST=ch-server.example.com
CLICKHOUSE_PORT=9000
CLICKHOUSE_DB=my_blockchain_db
CLICKHOUSE_USER=blockchain_user
CLICKHOUSE_PASSWORD=REPLACE_WITH_SECURE_PASSWORD

# Import Settings
DATA_DIR=/path/to/data/directory
LOG_DIR=/var/log/blockchain-import
SQL_DIR=/etc/blockchain/sql
ERROR_TOLERANCE=5

# Performance Settings
MAX_INSERT_THREADS=4
MAX_MEMORY_USAGE=5000000000
MAX_INSERT_BLOCK_SIZE=524288
